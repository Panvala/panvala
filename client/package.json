{
  "name": "panvala-client",
  "version": "0.0.1",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "storybook": "start-storybook -p 6006 -s ./ -c .storybook",
    "test": "jest",
    "update:contract-types": "typechain --target=ethers --outDir ./types '../packages/panvala-utils/abis/**/*.json'",
